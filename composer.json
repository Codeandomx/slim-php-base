{
    "name": "slim-template",
    "description": "slim php base",
    "require": {
        "monolog/monolog": "^1.24",
        "slim/slim": "^3.12",
        "slim/twig-view": "^2.4"
    },
    "autoload": {
        "psr-4": {
          "App\\": "src"
        }
      },
      "autoload-dev": {
        "psr-4": {
          "App\\Test\\": "tests"
        }
      },
      "config": {
        "sort-packages": true
      }
}
